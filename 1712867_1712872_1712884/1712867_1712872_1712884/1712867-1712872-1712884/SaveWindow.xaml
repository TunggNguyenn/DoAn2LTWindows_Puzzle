        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1712867_1712872_1712884"
        mc:Ignorable="d"
      Title="SaveWindow" Height="200" Width="400">
    <Canvas  Background="GreenYellow" Margin="0,0,3.333,-0.333">
        <TextBlock Text="File name: " FontWeight="Black" Canvas.Left="86" Canvas.Top="70"></TextBlock>
        <TextBox Name="SavedFileNameTextBox" Width="150" Height="25" Canvas.Left="155" Canvas.Top="70"></TextBox>
        <Button Content="Save"  Name="SaveButton" Click="SaveButton_Click" IsDefault="True" Width="80" Height="25" Canvas.Left="86" Canvas.Top="116"></Button>
        <Button Content="Cancel" Name="CalcelButton" Click="CalcelButton_Click" IsCancel="True" Width="80" Height="25" Canvas.Left="225" Canvas.Top="116"></Button>

    </Canvas>
</Window>

